{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "name": "post",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "populateCreatorFields": true,
    "publicAttributes": [
      "created_at",
      "updated_by",
      "created_by"
    ],
    "draftAndPublish": false
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "string"
    },
    "description": {
      "type": "string",
      "default": "Sed ut perspiciatis unde omnis iste natus error sit voluptatem accusantium doloremque laudantium"
    },
    "thumbnail": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "categories": {
      "collection": "category",
      "via": "posts"
    },
    "related_posts": {
      "collection": "post"
    },
    "content": {
      "type": "richtext"
    },
    "isFeature": {
      "type": "boolean",
      "default": false
    },
    "isActive": {
      "type": "boolean",
      "default": true
    },
    "collections": {
      "collection": "collection",
      "via": "posts"
    },
    "tags": {
      "type": "string"
    },
    "wide_thumbnail": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "destinos": {
      "via": "posts",
      "collection": "destino"
    },
    "pais": {
      "type": "string",
      "required": false
    }
  }
}
